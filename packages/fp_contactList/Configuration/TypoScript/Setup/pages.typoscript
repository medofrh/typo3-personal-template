page = PAGE

page.meta {
    viewport = width=device-width, initial-scale=1.0, shrink-to-fit=no
}

page.shortcutIcon = EXT:fp_contactlist/Resources/Public/Images/favicon.ico

page.includeCSS {
    bootstrap-icons = https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css
    bootstrap-icons.external = 1
    styles = EXT:fp_contactlist/Resources/Public/Css/styles.scss
    bootstrap.outputfile = typo3temp/bootstrap/fp_contactlist/style.css
}

page.includeJSFooter {
    bootstrap = https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js
    bootstrap.external = 1
    scripts = EXT:fp_contactlist/Resources/Public/Js/scripts.js
}

page.10 = FLUIDTEMPLATE
page.10 {
    layoutRootPath = EXT:fp_contactlist/Resources/Private/Layouts/
    partialRootPath = EXT:fp_contactlist/Resources/Private/Partials/

    #file = EXT:fp_contactlist/Resources/Private/Templates/Default.html
    file.stdWrap.cObject = CASE
    file.stdWrap.cObject {
        key.data = pagelayout

        pagets__layoutwithheaderandcontent = TEXT
        pagets__layoutwithheaderandcontent.value = EXT:fp_contactlist/Resources/Private/Templates/Default.html

    }
 
    variables {
        content0 < styles.content.get
    }
}